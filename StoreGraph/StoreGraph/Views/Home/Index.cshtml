@model StoreGraph.Models.ModelsModel

@{
    ViewBag.Title = "Home Page";
}

@using (Html.BeginForm("RenderGraph", "Home"))
{
    @Html.DropDownListFor(id => id.StoreID.SelectedStoreID, new SelectList(ViewBag.StoreIDs))
    @Html.DropDownListFor(floor => floor.StoreFloor.SelectedStoreFloor, new SelectList(ViewBag.StoreFloors))
    
    <input type="submit"  value="Get Average Bay Profit For Store"/>
}